{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "rewrite": {
          "source": "/api/*",
          "destination": "/api/index.js?name=:name"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/api/*",
      "dest": "api/index.js"
    }
  ]
}

